<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ASBC.ASBCActivity">
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_marginTop="30dp"
        android:id="@+id/tvHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Broardcast receiver"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_marginTop="30dp"
        android:id="@+id/tvHomeReceiver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Broardcast receiver"
        app:layout_constraintTop_toBottomOf="@+id/tvHome"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_marginTop="30dp"
        android:id="@+id/tvHomeCP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ContentProvider receiver"
        app:layout_constraintTop_toBottomOf="@+id/tvHomeReceiver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_marginTop="30dp"
        android:id="@+id/tvHomeCPReceiver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ContentProvider receiver"
        app:layout_constraintTop_toBottomOf="@+id/tvHomeCP"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>





</androidx.constraintlayout.widget.ConstraintLayout>